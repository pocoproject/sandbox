logging.loggers.l1.name = ServletTestLogger
logging.loggers.l1.channel.class = SplitterChannel
logging.loggers.l1.channel.channel1 = c1
logging.loggers.l1.channel.channel2 = c2
logging.loggers.l1.level = trace
logging.channels.c1.class = ConsoleChannel
logging.channels.c1.formatter = f1
logging.channels.c2.class = FileChannel
logging.channels.c2.path = ${system.env.POCO_BASE}/Servlet/runtime/webapps/ServletTest/WEB-INF/log/ServletTest.log
logging.channels.c2.formatter= f2
logging.channels.c2.times= local
logging.channels.c2.rotation= 00:00:
logging.channels.c2.archive= timestamp
logging.channels.c2.purgeAge= 1 months
logging.formatters.f1.class = PatternFormatter
logging.formatters.f1.pattern = [%Y-%m-%d %H:%M:%S.%c] [%q] %t
logging.formatters.f1.times = local
logging.formatters.f2.class = PatternFormatter
logging.formatters.f2.pattern = [%Y-%m-%d %H:%M:%S.%c] [%q] %t
logging.formatters.f2.times = local